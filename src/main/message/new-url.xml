<?xml version="1.0"?>
<!--
  Sample XML for URL creation/modification notifications.
-->
<notifications>
  <notification template="new-url" announcement="true" description="Creation of new URL as announcement"
                subject="[acme-test] URL Created: My URL title">
    <uri id="456" scheme="https" host="www.my-external-host.com" port="443" path="/my-external-url.html"
         decodedpath="/my-external-url.html" external="true" created="2013-08-26T12:11:59+10:00"
         title="My URL title">
      <displaytitle>My URL title</displaytitle>
      <description>My URL description.</description>
    </uri>
    <group id="123" name="myproject-mygroup" owner="My Organisation" description="My Own Group">
      <message>Welcome to the group!</message>
    </group>
  </notification>
  <notification template="new-url" description="Creation of new URL"
                subject="[acme-test] URL Created: My URL title">
    <uri id="456" scheme="https" host="www.my-external-host.com" port="443" path="/my-external-url.html"
         decodedpath="/my-external-url.html" external="true" created="2013-08-26T12:11:59+10:00"
         title="My URL title">
      <displaytitle>My URL title</displaytitle>
      <description>My URL description.</description>
    </uri>
    <group id="123" name="myproject-mygroup" owner="My Organisation" description="My Own Group">
      <message>Welcome to the group!</message>
    </group>
  </notification>
  <notification template="new-url" announcement="true" modified="true"
                description="Modification of URL as announcement"
                subject="[acme-test] URL Modified: My URL title">
    <uri id="456" scheme="https" host="www.my-external-host.com" port="443" path="/my-external-url.html"
         decodedpath="/my-external-url.html" external="true" created="2013-08-26T12:11:59+10:00"
         title="My URL title">
      <displaytitle>My URL title</displaytitle>
      <description>My URL description.</description>
    </uri>
    <group id="123" name="myproject-mygroup" owner="My Organisation" description="My Own Group">
      <message>Welcome to the group!</message>
    </group>
  </notification>
  <notification template="new-url" modified="true" description="Modification of URL"
                subject="[acme-test] URL Modified: My URL title">
    <uri id="456" scheme="https" host="www.my-external-host.com" port="443" path="/my-external-url.html"
         decodedpath="/my-external-url.html" external="true" created="2013-08-26T12:11:59+10:00"
         title="My URL title">
      <displaytitle>My URL title</displaytitle>
      <description>My URL description.</description>
    </uri>
    <group id="123" name="myproject-mygroup" owner="My Organisation" description="My Own Group">
      <message>Welcome to the group!</message>
    </group>
  </notification>
</notifications>